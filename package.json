{
  "name": "supabase-firebird-sync",
  "version": "1.0.0",
  "description": "Servicio de sincronizaci√≥n entre Supabase y Firebird para facturas aprobadas",
  "main": "src/index.js",
  "bin": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "sync-thirds": "node src/scripts/syncThirdParties.js",
    "sync-thirds-full": "node src/scripts/syncThirdParties.js full",
    "sync-thirds-stats": "node src/scripts/syncThirdParties.js stats",
    "sync-accounts": "node src/scripts/syncChartOfAccounts.js",
    "sync-accounts-full": "node src/scripts/syncChartOfAccounts.js full",
    "sync-accounts-stats": "node src/scripts/syncChartOfAccounts.js stats",
    "sync-accounts-config": "node src/scripts/syncChartOfAccounts.js config",
    "test-account-ranges": "node src/scripts/testAccountRanges.js",
    "test-account-exclusion": "node src/scripts/testAccountExclusion.js",
    "sync-products": "node src/scripts/syncProducts.js",
    "sync-products-full": "node src/scripts/syncProducts.js full",
    "sync-products-stats": "node src/scripts/syncProducts.js stats",
    "sync-products-config": "node src/scripts/syncProducts.js config",
    "test-recovery": "node src/scripts/testRecovery.js",
    "test-invc-config": "node src/scripts/testInvcConfiguration.js",
    "test-dates": "node src/scripts/testDateHandling.js",
    "test-project-activity": "node src/scripts/testProjectActivity.js",
    "test-document-type": "node src/scripts/testDocumentType.js",
    "test-user-filter": "node src/scripts/testUserFilter.js",
    "diagnostic": "node src/scripts/diagnosticIssues.js",
    "test-pinecone": "node src/scripts/testPineconeSync.js",
    "sync-products-to-pinecone": "node src/scripts/syncProductsToPinecone.js",
    "search-products": "node src/scripts/searchProducts.js",
    "test-third-party-creation": "node src/scripts/testThirdPartyCreation.js",
    "test-auto-third-party-flow": "node src/scripts/testAutoThirdPartyCreationFlow.js",
    "test-third-party-nit": "node src/scripts/testThirdPartyNitReturn.js",
    "test-realtime": "node src/scripts/testRealtimeConnection.js",
    "diagnose-third-party": "node src/scripts/diagnoseThirdPartyIssue.js",
    "test-extract-idn": "node src/scripts/testExtractIdN.js",
    "test-inventory-sync": "node src/scripts/testInventorySync.js",
    "test-direct-firebird": "node src/scripts/testDirectFirebirdInventory.js",
    "test-inventory-products": "node src/scripts/testInventoryWithProducts.js",
    "test-auto-match": "node src/scripts/testAutoMatchProducts.js",
    "test-auto-match-service": "node src/scripts/testAutoMatchService.js",
    "test-auto-match-full": "node src/scripts/testAutoMatchWithSync.js",
    "test-learning": "node src/scripts/testLearningSystem.js",
    "verify-auto-match": "node src/scripts/verifyAndFixAutoMatch.js",
    "test-acct-versioning": "node scripts/test-acct-versioning.js",
    "build": "build-sea.bat",
    "build:sea": "build-sea.bat",
    "build:complete": "build-sea.bat",
    "build:legacy": "pkg . --targets node18-win-x64 --output dist/supabase-firebird-sync.exe",
    "build:all": "pkg . --targets node18-win-x64,node18-linux-x64 --out-path dist",
    "build:installers": "pkg scripts/install-service.js --targets node18-win-x64 --output dist/install-service.exe && pkg scripts/uninstall-service.js --targets node18-win-x64 --output dist/uninstall-service.exe && pkg scripts/encrypt-env.js --targets node18-win-x64 --output dist/encrypt-env.exe",
    "encrypt-env": "node scripts/encrypt-env.js",
    "decrypt-env": "node scripts/decrypt-env.js",
    "install-service": "node scripts/install-service.js",
    "uninstall-service": "node scripts/uninstall-service.js",
    "install-multi-instance": "node scripts/install-multi-instance.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "supabase",
    "firebird",
    "sync",
    "invoices",
    "realtime"
  ],
  "author": "PuroDelphi",
  "license": "MIT",
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.3",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.13.2",
    "dotenv": "^16.3.1",
    "node-firebird": "^1.1.7",
    "node-windows": "^1.0.0-beta.8",
    "winston": "^3.11.0"
  },
  "optionalDependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nexe": "^4.0.0-rc.6",
    "nodemon": "^3.0.2",
    "pkg": "^5.8.1",
    "postject": "^1.0.0-alpha.6"
  },
  "pkg": {
    "assets": [
      "node_modules/node-firebird/**/*",
      "logs/**/*"
    ],
    "scripts": [
      "src/**/*.js"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}
